<% if !@video.errors.blank? %>
	$("#bookmark_alert").css('color','#FF0000').text("<%= @video.errors.full_messages.uniq.join(', ').html_safe  %>");
<% else %>
	$("#bookmark_form_content").html("<%= j(render 'admin/topics/bookmark_form')%>");
	$("#bookmark_alert").text("Bookmark have been Submitted.");
<% end %>
$("#bookmark_button").button('reset');