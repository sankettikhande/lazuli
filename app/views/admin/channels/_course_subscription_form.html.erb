<div class="col-sm-2">
	<label class="control-label"> Available Subscription: </label>
</div>
<div class="col-sm-10">
	<div class="checkbox-list">
		<% i = 0 %>
		<%= f.fields_for :course_subscriptions do |course_subscription_form| %>
			<div class="col-sm-5">
				<%= course_subscription_form.check_box :subscription_id %>
				<label class="checkbox-inline"> <%= @subscriptions[i].name  %> </label>
			</div>
			<% i += 1 %>
		<% end %>
	</div>
</div>
<div class="seperator10"></div>