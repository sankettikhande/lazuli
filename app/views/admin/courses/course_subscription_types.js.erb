<% if params[:subscription_id] && params[:permission_id] %>
	$('#<%= params[:subscription_id] %>').html("<%= escape_javascript(render 'subscription_types') %>");
	$('#<%= params[:permission_id] %>').html("<%= escape_javascript(render 'user_permissions') %>");
	$('input[type="checkbox"]').uniform();
<% elsif @is_trial_subscription %>
	$('.trial_subscription').show()
<% else %>
	$('.trial_subscription').hide()
<% end %>

