							<div class="col-sm-2 text-right"><h5>Sequence Number</h5></div>
							<div class="col-sm-1"><input class="form-control" type="number" min="1" name="seqNum1" id="seqNum1"></div>
							<div class="col-sm-1 nopadding">
								<div class="btn-group">
									<button type="button" class="btn btn-grey"><i class="fa fa-ban"></i></button>
									<div class="btn-group">
										<button type="button" class="btn btn-grey">
										<i class="fa fa-chevron-down"></i>
										</button>
									</div>
								</div>
							</div>

						</div>
				</div>
			</div>
			<div id="collapse_1" class="panel-collapse collapse">
				<div class="panel-body form-group">
					<div class="row">
						<div class="col-sm-2">
							<label class="control-label">Title of Video</label>
						</div>
						<div class="col-sm-4">
							<%= f.text_field :title, :class => "form-control"%>
							<!-- <input class="form-control" type="text" name="VideoTitle1" id="VideoTitle1"> -->
						</div>
						<div class="col-sm-2">
							<label class="control-label">Thumbnail</label>
						</div>
						<div class="col-sm-4">

							<div class="fileupload fileupload-new" data-provides="fileupload">
								<div class="fileupload-new thumbnail" style="width: 180px; height: 135px;">
									<img src="http://www.placehold.it/200x150/EFEFEF/AAAAAA&amp;text=no+image" alt="" title="Attached image thumbnail is supported in Latest Firefox, Chrome, Opera, Safari and Internet Explorer 10 only">
								</div>
								<div class="fileupload-preview fileupload-exists thumbnail" style="max-width: 180px; max-height: 135px; line-height: 20px;"></div>
								<div>
									<span class="btn purple btn-file">
									<span class="fileupload-new"><i class="fa fa-paper-clip"></i> Edit</span>
									<span class="fileupload-exists"><i class="fa fa-undo"></i> Change</span>
										<%= f.file_field :image, :class => "default"%>
									</span>
									<a href="#" class="btn red fileupload-exists" data-dismiss="fileupload"><i class="fa fa-trash-o"></i> Remove</a>
								</div>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="col-sm-2">
							<label class="control-label">Description</label>
						</div>
