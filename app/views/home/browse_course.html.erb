<section class="btns-crumbs visible-md visible-lg">
  <div class="row">
    <%= render 'shared/user_menu' if user_signed_in? %>
  </div>
</section>
<div class="seperator5"></div>
<div class="seperator5"></div>
<section class="all-courses indigo-back">
  <div class="row">
    <div class="col-sm-12">
      <div class="row">
        <div class="col-xs-12 col-sm-6">
          <h4>Browse Our Library</h4>
        </div>
        <div class="col-xs-12 col-sm-3 col-sm-offset-3">
          <div class="form-group">
            <input type="search" class="form-control" placeholder="Search Channels" id="search_browse_courses">
          </div>
        </div>
      </div>
      <div class="partial-channel-list-box" id="partial_channel_list">
        <%= render 'home/partial_channels', :channels => @channels %>
      </div>
    </div>
  </div>
</section>

<%= javascript_include_tag "application/main_app/slim_scroll.js" %>
<%= javascript_include_tag "application/main_app/browse_course.js" %>